    />

    <script>
      // this code is the most basic way i could find to force you into https
      // if you land on the page using http. with rails and node there is complicated
      // backend stuff but this was easy enough and seems to work.
      var prodHost = 'web.linqapp.com'
      var newProdHost = 'www.linqapp.com'
      var linqAppDotCom = 'linqapp.com'
      var stagingHost = 'staging-web.linqapp.com'
      if (
        (prodHost === window.location.host ||
          stagingHost === window.location.host ||
          newProdHost === window.location.host ||
          linqAppDotCom === window.location.host) &&
        window.location.protocol != 'https:'
      ) {
        window.location.protocol = 'https'
      }
    </script>
    <script type="module" crossorigin src="/assets/index-DveBN9AF.js"></script>
